{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "statusCode": 200
    }
  ],
  "env": {
    "VITE_BACKEND_URL": "http://localhost:3000",
    "VITE_API_BASE_URL": "/api",
    "VITE_ENV": "production"
  }
}
